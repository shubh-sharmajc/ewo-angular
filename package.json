{
  "name": "ewo-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --live-reload false --proxy-config proxy.conf.json  --host 0.0.0.0 --port 4203",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats.json",
    "build": "ng build",
    "build:prod": "ng build --prod --aot --build-optimizer=true",
    "build:dev": "ng build --configuration=dev --stats-json --aot --build-optimizer=true",
    "build:staging": "ng build --configuration=staging --aot --build-optimizer=true",
    "lint": "ng lint ewo-front-demo",
    "build:dev-client-and-server-bundles": "npm run build:dev && ng run ewo-front:server",
    "build:dev-ssr": "npm run build:dev-client-and-server-bundles && npm run webpack:server",
    "build:staging-client-and-server-bundles": "npm run build:staging && ng run ewo-front:server",
    "build:staging-ssr": "npm run build:staging-client-and-server-bundles && npm run webpack:server",
    "build:prod-client-and-server-bundles": "npm run build:prod && ng run ewo-front:server",
    "build:prod-ssr": "npm run build:prod-client-and-server-bundles && npm run webpack:server",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "compile:server": "tsc -p server.tsconfig.json",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server.js"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^7.3.6",
    "@angular/animations": "^7.0.0",
    "@angular/cdk": "^7.3.5",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/material": "^7.3.5",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@angular/upgrade": "^7.0.0",
    "@ng-bootstrap/ng-bootstrap": "^4.2.2",
    "@ng-idle/core": "^8.0.0-beta.4",
    "@ng-idle/keepalive": "^8.0.0-beta.4",
    "@ng-select/ng-select": "^2.20.0",
    "@ngrx/store": "^7.4.0",
    "@nguniversal/common": "^7.1.1",
    "@nguniversal/express-engine": "^7.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.0",
    "angularx-social-login": "^2.2.1",
    "bootstrap": "^4.5.2",
    "bs-stepper": "^1.7.0",
    "classlist.js": "^1.1.20150312",
    "compression": "^1.7.4",
    "cookieconsent": "^3.1.1",
    "core-js": "^2.5.4",
    "domino": "^2.1.4",
    "fs": "^0.0.1-security",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.29.0",
    "ng-in-viewport": "^6.0.3",
    "ng-recaptcha": "^5.0.0",
    "ngx-bootstrap": "^5.6.1",
    "ngx-cookie": "^4.1.2",
    "ngx-cookieconsent": "^2.2.3",
    "ngx-drag-scroll": "^7.0.0-beta.4",
    "ngx-hm-carousel": "^1.6.5-bata.1",
    "ngx-image-cropper": "^3.1.7",
    "ngx-mask": "^8.2.0",
    "ngx-scroll-event": "^1.0.8",
    "ngx-spinner": "7.1.1",
    "object-path": "^0.11.4",
    "rxjs": "^6.5.4",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.6",
    "@angular/cli": "^7.0.0",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/platform-server": "^7.0.0",
    "@angular/language-service": "~7.0.0",
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-typescript": "7.3.3",
    "@ngrx/store-devtools": "^9.1.0",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^25.1.2",
    "@types/jquery": "^3.3.34",
    "@types/node": "^13.7.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.14.0",
    "protractor": "~5.4.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "webpack-cli": "^3.3.11"
  }
}
